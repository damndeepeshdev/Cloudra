{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:event:default",
    "opener:default",
    "dialog:default",
    "fs:default",
    {
      "identifier": "fs:scope",
      "allow": [
        "$TEMP/**"
      ]
    },
    "updater:default",
    "process:allow-restart"
  ]
}